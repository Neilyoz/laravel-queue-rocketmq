{
    "name": "neilyoz/laravel-queue-rocketmq",
    "description": "laravel queue rocketmq implements",
    "authors": [
        {
            "name": "neilyoz",
            "email": "neilyoz@foxmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/queue": "^8.0",
        "ext-json": "*",
        "ext-curl": "*",
        "aliyunmq/mq-http-sdk": "^1.0",
        "guzzlehttp/guzzle": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0.0",
        "vlucas/phpdotenv": "^3.3",
        "mockery/mockery": "^1.2.3"
    },
    "autoload": {
        "psr-4": {
            "Neilyoz\\LaravelQueueRocketMQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neilyoz\\LaravelQueueRocketMQ\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Neilyoz\\LaravelQueueRocketMQ\\LaravelQueueRocketMQServiceProvider"
            ]
        }
    }
}
